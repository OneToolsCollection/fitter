{
  "items": [
    {
      "name": "Current time",
      "connector_config": {
        "response_type": "json",
        "url": "https://time.is",
        "static_config": {
          "raw": [1,2,3,"{{{FromEnv=ENV_VALUE}}}",5,6,7,8,9]
        }
      },
      "model": {
        "array_config": {
          "item_config": {
            "field": {
              "type": "int"
            }
          }
        }
      },
      "trigger_config": {
        "scheduler_trigger": {
          "interval": 10
        }
      },
      "notifier_config": {
        "send_array_by_item": true,
        "console": {
          "only_result": true
        }
      }
    }
  ]
}